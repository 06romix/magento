<?xml version="1.0"?>
<config>
    <modules>
        <Plumrocket_FAQ>
            <version>0.1.0</version>
        </Plumrocket_FAQ>
    </modules>
    <frontend>
        <routers>
            <psfaq>
                <use>standard</use>
                <args>
                    <module>Plumrocket_FAQ</module>
                    <frontName>psfaq</frontName>
                </args>
            </psfaq>
        </routers>
        <layout>
            <updates>
                <psfaq>
                    <file>psfaq.xml</file>
                </psfaq>
            </updates>
        </layout>
    </frontend>
    <adminhtml>
        <adminhtml>
            <layout>
                <updates>
                    <psfaq>
                        <file>faq.xml</file>
                    </psfaq>
                </updates>
            </layout>
        </adminhtml>
    </adminhtml>
    <global>
        <models>
            <psfaq>
                <class>Plumrocket_FAQ_Model</class>
                <resourceModel>psfaq_resource</resourceModel>
            </psfaq>
            <psfaq_resource>
                <class>Plumrocket_FAQ_Model_Mysql4</class>
                <entities>
                    <post>
                        <table>plumrocket_faq_post</table>
                    </post>
                </entities>
            </psfaq_resource>
        </models>
        <helpers>
            <psfaq>
                <class>Plumrocket_FAQ_Helper</class>
            </psfaq>
        </helpers>
        <blocks>
            <psfaq>
                <class>Plumrocket_FAQ_Block</class>
            </psfaq>
        </blocks>
        <resources>
            <psfaq_setup>
                <setup>
                    <module>Plumrocket_FAQ</module>
                </setup>
            </psfaq_setup>
        </resources>
    </global>
    <admin>
        <routers>
            <faq>
                <use>admin</use>
                <args>
                    <module>Plumrocket_FAQ</module>
                    <frontName>admin_faq</frontName>
                </args>
            </faq>
        </routers>
    </admin>
</config>